<% provide(:title, '用户列表')
   provide(:search_act, users_path) %>

<div class="col-md-12">
  <%if @users.count > 0%>
  <table class="table">
    <tbody>
      <% for user in @users%>
      <tr>
	<td><%=link_to user.name, user_show_path(id:user.name)%></td>
      </tr>
      <%end%>
    </tbody>
  </table>
  <%else%>
  没有结果
  <%end%>
</div>
