<% provide(:title, '新建条目') %>

<div class="row">
    <div class="col-md-12">
      <%= render partial: 'shared/errors', object: @channel.errors %>
      <%= form_for @channel, url: contents_current do |f| %>
      <div class="form-group">
        <%= f.label :name, "名称" %>
        <%= f.text_field :name, class:"form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :description, "描述" %>
        <%= f.text_area :description, class:"form-control", rows:10 %>
      </div>
        <%= f.submit "确定", class:"btn btn-default" %>
      <% end %>
    </div>
</div>