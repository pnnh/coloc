<!DOCTYPE html>
<html>
<head>
    <title><%=@channel.title%></title>
    <%= render 'layouts/header' %>
</head>
<body>

<div class="container channels">
    <% for c in @channels%>
        <div class="post post-list-item">
            <h2>
                <%= link_to c.title, c %>
            </h2>
            <small>&nbsp;</small>
            <div class="entry">
                <div class="c_b_p_desc"><%= c.description %></div>
            </div>
        </div>
    <%end%>
</div>
<%= render 'layouts/footer' %>
</body>
</html>
