<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinksxb-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div class="container-fluid">
    <div class="row">
      <%= yield %>
    </div>
    <div class="row">
      <div class="col-md-12">
        <%= render 'layouts/footer' %>
	<%= debug(params) if Rails.env.development? %>
      </div>
    </div>
  </div>
</body>
</html>
